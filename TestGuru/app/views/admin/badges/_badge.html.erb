<tr>
  <td><%= badge.name %></td>
  <td><%= badge.rule %></td>
  <td><%= badge.value %></td>
  <td><%= image_tag("#{badge.image_url}") %></td>
  <td>
    <%= link_to t('.delete'), admin_badge_path(badge),
                method: :delete,
                data: { confirm: t('.confirm') } %>
  </td>
</tr>
